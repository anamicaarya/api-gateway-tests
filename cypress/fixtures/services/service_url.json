{
	"workspace": "default",
	"route_error": "no Route matched with those values",
	"service_with_url": {
		"name": "json_service",
		"url": "https://jsonplaceholder.typicode.com",
		"routes": [
			{
				"name": "json_service-strip",
				"path": "/json1",
				"resource": "/json1/todos/1",
				"strip_path": true,
				"methods": [
					"GET",
					"POST",
					"PUT",
					"DELETE"
				],
				"response": {
					"status_code": 200,
					"headers": ["X-Kong-Request-Id", "X-Kong-Upstream-Latency", "X-Kong-Proxy-Latency"],
	    			"stripped_path": "/todos"
				}
			},
			{
				"name": "json_service-nostrip",
				"path": "/json2",
				"resource": "/json2/todos",
				"strip_path": false,
				"limit": 2,				
				"methods": [
					"GET",
					"POST",
					"PUT",
					"DELETE"
				],
				"response": {
					"status_code": 404
				}
			}
	    ],
		"plugin":{
			"name": "key-auth",
			"type": "key-auth-card",
			"route": "example_route",
			"scoped": true
		}
	},
	"service_with_protocol": {
		"name": "echo_service",
		"url": "https://httpbin.konghq.com",
		"protocol": "http",
		"host": "echo",
		"port": 80,
		"path": "/",
		"route": {
			"config_type": "basic",
			"name": "r-echo",
			"path": "/echo",
			"strip_path": "enabled"
		},
		"plugin":{
			"name": "key-auth",
			"type": "key-auth-card",
			"route": "r-echo",
			"scoped": true
		}
	},

	"services": [
		{
			"name": "example_service",
			"entity": "gateway-service",
			"entity_text": "New gateway service",
			"url": "https://httpbin.konghq.com",
			"protocol": "https",
			"host": "httpbin.konghq.com",
			"post": 443,
			"route": {
				"config_type": "basic",
				"name": "example_route",
				"path": "/mock",
				"methods": [
					"GET",
					"POST",
					"PUT",
					"DELETE"
				]
			},
			"plugin":{
				"name": "key-auth",
				"type": "key-auth-card",
				"route": "example_route",
				"scoped": true
			}

		},
		{
			"name": "example_service1",
			"url": "https://httpbin.konghq.com"
		}
	],
    "consumer": {
    	"username": "K2G",
    	"custom_id": 1,
    	"credentials": {
    		"key": "kongsecret"
    	}

    }	
}